<h1 style="text-align: center;">Welcome to the Donor's Page</h1>

<table class="table table-striped table-hover ">
    <thead>
    <tr>
        <th>Organization</th>
        <th>Address</th>
        <th>Phone Number</th>
        <th>EMail</th>
        <th>User Type</th>
        <th>Items Needed</th>
    </tr>
    </thead>
    <tbody>
    {{#each models}}
    <tr>
        <td>{{this.attributes.orgName}}</td>
        <td>{{this.attributes.address}}</td>
        <td>{{this.attributes.phoneNumber}}</td>
        <td>{{this.attributes.email}}</td>
        <td>{{this.attributes.Type}}</td>
        <td>{{this.attributes.itemsNeeded}}</td>
    </tr>
    {{/each}}
    </tbody>
</table>